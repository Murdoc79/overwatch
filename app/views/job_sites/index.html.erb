<p id="notice"><%= notice %></p>

<h1>Job Sites</h1>

<table>
  <thead>
    <tr>
      <th>Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @job_sites.each do |job_site| %>
      <tr>
        <td><%= job_site.address %></td>
        <td><%= link_to 'Show', job_site %></td>
        <td><%= link_to 'Edit', edit_job_site_path(job_site) %></td>
        <td><%= link_to 'Destroy', job_site, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Jobsite', new_job_site_path %>
<br>
<%= link_to "Main", 'reports/main' %>
